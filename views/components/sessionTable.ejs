<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>Question</th>
      <th>Answer</th>
    </tr>
  </thead>
  <tbody>
    <!-- display session table with content -->
    <% for(let index = 0; index < questionsAnswers.questions.length; index++) {
    let question = questionsAnswers.questions[index]; 
    let answer = questionsAnswers.answers[index]; %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= question %></td>
      <td><%= answer %></td>
    </tr>
    <% } %>
  </tbody>
</table>
